Name: <%= text_field_tag "location[contacts][#{contact_idx}][name]", contact[:name] %>
<br>
Email: <%= text_field_tag "location[contacts][#{contact_idx}][email]", contact[:email] %>
<br>
<% if contact[:phone] %>
  <% contact[:phone].each_with_index do |phone, j| %>
    <%= render :partial => "phone_number_form", :locals => {:phone => phone, :contact_idx => contact_idx, :phone_idx => j } %>
  <% end %>
<% end %>
<br>
